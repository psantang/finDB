<?xml version='1.0' encoding='utf-8'?>
<widget id="com.paulsantangelo.finDB" version="2.0.9" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>finDB</name>
    <description>
        The only mobile app providing all stock settings for high end water skis with the ability to save your custom ski settings for an unlimited number of skis.
    </description>
    <author email="paul@paulsantangelo.com" href="http://www.paulsantangelo.com">
        Paul Santangelo Software, Inc.
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="phonegap-version" value="cli-7.1.0" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
