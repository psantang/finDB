<?xml version='1.0' encoding='utf-8'?>
<widget id="com.paulsantangelo.finDB" version="0.0.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>finDB</name>
    <description>
        FinDB is a database lookup for most popular water ski stock settings.
    </description>
    <author email="paul@paulsantangelo.com" href="http://www.paulsantangelo.com">
        Paul Santangelo Software, Inc.
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="www/res/icon/ios/icon.png" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
