<?xml version='1.0' encoding='utf-8'?>
<widget id="com.paulsantangelo.finDB" version="2.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>finDB V2</name>
    <description>
        FinDB V2 is a database lookup for most popular water ski stock settings.
    </description>
    <author email="paul@paulsantangelo.com" href="http://www.paulsantangelo.com">
        Paul Santangelo Software, Inc.
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon height="1024" src="www/res/icon/ios/icon-1024x1024.png" width="1024" />
    <preference name="phonegap-version" value="cli-7.1.0" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
